@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUserRole<string>>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<p>
	<a asp-action="Create">Create New</a>
</p>
<table id="myTable" class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RoleId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @ViewBag.Users[item.UserId]
                @*@Html.DisplayFor(modelItem => item.UserId)*@
            </td>
			<td>
				@ViewBag.Roles[item.RoleId]
				@*@Html.DisplayFor(modelItem => item.RoleId)*@
			</td>
            <td>
                @*<a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |*@
                @*<a asp-action="Details" asp-route-id="@item.Id">Details</a>*@ |
                <a asp-action="Delete" asp-route-UserId="@item.UserId" asp-route-RoleId="@item.RoleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
